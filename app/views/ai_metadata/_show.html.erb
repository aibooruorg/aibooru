<table class="striped ai-metadata-table">
  <tbody>
    <tr>
      <th>Prompt</th>
      <td><%= ai_metadata.prompt.presence || unknown_field %></td>
    </tr>
    <tr>
      <th>Negative prompt</th>
      <td><%= ai_metadata.negative_prompt.presence || unknown_field %></td>
    </tr>
    <% ai_metadata.sorted_parameters.each do |name, value| %>
      <tr>
        <th><%= name %></th>
        <td>
          <% if name == "Model Hash" && value.present? %>
            <%= link_to value, ai_models_path(search: { model_hash: value }) %>
          <% else %>
            <%= value.presence || unknown_field %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
